	<profiles>
		<profile>
			<id>dev</id>
			<activation>
				<activeByDefault>true</activeByDefault>
			</activation>
			<properties>
				<server>dev</server>
			</properties>
		</profile>
		<profile>
			<id>hml</id>
			<properties>
				<server>hml</server>
			</properties>
		</profile>
		<profile>
			<id>prd</id>
