		</profile>
	</profiles>

	<build>
		
		<sourceDirectory>src</sourceDirectory>
		
		<filters>
            <filter>src/seia-${server}.properties</filter>
        </filters>
		
		<resources>
			<resource>
				<directory>src</directory>
				<includes>
					<include>*.properties</include>
					<include>*.jasper</include>
				</includes>
				<excludes>
					<exclude>**/*.java</exclude>
				</excludes>
			</resource>
			<resource>
				<directory>src</directory>
				<filtering>true</filtering>
				<includes>
					<include>log4j.properties</include>
					<include>seia.properties</include>
					<include>seia-dev.properties</include>
					<include>seia-hml.properties</include>
					<include>seia-prd.properties</include>
					<include>seia-sml.properties</include>
					<include>seia-trm.properties</include>
				</includes>
			</resource>
			<resource>
				<directory>src/META-INF</directory>
				<targetPath>META-INF</targetPath>
			</resource>
			<resource>
				<directory>src/br/gov/ba/seia/reports</directory>
				<targetPath>br/gov/ba/seia/reports</targetPath>
				<!-- <excludes>
					<exclude>**/*.jrxml</exclude>
				</excludes> -->
			</resource>
		</resources>
			<plugins>
				<plugin>
					<artifactId>maven-compiler-plugin</artifactId>
					<version>3.8.0</version>
					<configuration>
						<source>1.6</source>
						<target>1.6</target>
					</configuration>
				</plugin>
				<plugin>
					<artifactId>maven-war-plugin</artifactId>
					<version>3.2.1</version>
					<configuration>
						<warSourceDirectory>WebContent</warSourceDirectory>
					</configuration>
				</plugin>
				<plugin>
					<groupId>org.codehaus.mojo</groupId>
					<artifactId>jasperreports-maven-plugin</artifactId>
					<version>1.0-beta-2</version>
					<executions>
						<execution>
							<phase>compile</phase>
							<goals>
								<goal>compile-reports</goal>
							</goals>
							<configuration>
								<sourceDirectory>src/br/gov/ba/seia/reports/</sourceDirectory>
								<sourceFileExt>.jrxml</sourceFileExt>
								<outputDirectory>src/br/gov/ba/seia/reports/</outputDirectory>
								<outputFileExt>.jasper</outputFileExt>
								<compiler>net.sf.jasperreports.compilers.JRGroovyCompiler</compiler>
							</configuration>
						</execution>
					</executions>
					<dependencies>
						<dependency>
							<groupId>org.codehaus.mojo</groupId>
							<artifactId>jasperreports-maven-plugin</artifactId>
							<version>1.0-beta-2</version>
							<exclusions>
								<exclusion>
									<groupId>jasperreports</groupId>
									<artifactId>jasperreports</artifactId>
								</exclusion>
							</exclusions>
						</dependency>
						<dependency>
							<groupId>net.sf.jasperreports</groupId>
							<artifactId>jasperreports</artifactId>
							<version>5.0.4</version>
							<exclusions>
								<exclusion>
									<groupId>commons-logging</groupId>
									<artifactId>commons-logging</artifactId>
								</exclusion>
							</exclusions>
						</dependency>
						<dependency>
							<groupId>org.codehaus.groovy</groupId>
							<artifactId>groovy-all</artifactId>
							<version>2.0.1</version>
						</dependency>
					</dependencies>
				</plugin>
			</plugins>
	</build>
	
