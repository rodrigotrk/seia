	<queryString>
		<![CDATA[select distinct
	le.ide_lac_erb,
	le.dsc_nome_erb,
	le.cod_erb,
	le.vlr_area_total_terreno,
	le.vlr_altura_torre,
	le.vlr_altura_antena_base,
	le.vlr_menor_dist_limite,
	dsc_nome_edificacao,
	le.vlr_altura_edificacao,
	le.vlr_altura_maior, le.vlr_altura_menor, le.vlr_altura_antena_edificacao, le.dsc_outros_tipo_delimitacao, le.dsc_nome_estabelecimento , le.dsc_atividade_estabelecimento , le.vlr_potencia_transmissor,
	le.vlr_frequencia_utilizada ,  lc.ide_documento_obrigatorio , le.ind_compartilhado, le.vlr_azimute_antena_base ,
	lc.ide_requerimento ,  rq.nom_contato ,  rq.num_telefone, rq.ide_orgao,  rq.num_requerimento,  rq.ide_endereco_contato,  rq.ind_declaracao, rq.ide_tipo_requerimento, rqu.ide_requerimento_unico,
	rqu.ide_fase_empreendimento,
	pt.ide_porte,  pt.sgl_porte,  coalesce (pt.nom_porte,'Não Identificado') as nom_porte,tm.ide_tipo_modalidade_erb,tm.dsc_tipo_modalidade_erb , rp.ide_tipo_pessoa_requerimento,p.ide_pessoa,
	pj.ide_pessoa_juridica,pj.ide_natureza_juridica ,initcap(pj.nom_razao_social) as nom_razao_social ,pj.nom_nome_fantasia , pj.dtc_abertura, pj.num_cnpj, pj.num_inscricao_municipal, pj.num_inscricao_estadual ,
	pf.ide_pessoa_fisica ,  pf.ide_estado_civil,  pf.ide_escolaridade ,  pf.ide_ocupacao ,  pf.ide_pais ,  initcap(pf.nom_pessoa) as nom_pessoa,  pf.tip_sexo ,  pf.dtc_nascimento ,  pf.des_naturalidade,  pf.nom_pai ,
	pf.nom_mae  ,  pf.num_cpf, er.ide_empreendimento_requerimento,em.ide_empreendimento,  em.ide_localizacao_geografica ,  em.nom_empreendimento,  em.val_investimento ,
	em.qtd_funcionarios ,  em.num_cadastro_tecnico_federal ,  em.dtc_validade_ctf,  em.ind_correspondencia,
	ereq.num_endereco as num_endereco_req,ereq.des_complemento as des_complemento_req,lreq.nom_logradouro as nom_logradouro_req,
	tlreq.nom_tipo_logradouro as nom_tipo_logradouro_req,tlreq.sgl_tipo_logradouro as sgl_tipo_logradouro_req,
	brreq.nom_bairro as nom_bairro_req,mpreq.nom_municipio as nom_municipio_req,etreq.nom_estado as nom_estado_req,
	preq.nom_pais as nom_pais_req,
	eemp.num_endereco as num_endereco_emp,eemp.des_complemento as des_complemento_emp,lemp.nom_logradouro as nom_logradouro_emp,
	tlemp.nom_tipo_logradouro as nom_tipo_logradouro_emp,tlemp.sgl_tipo_logradouro as sgl_tipo_logradouro_emp,
	bremp.nom_bairro as nom_bairro_emp,mpemp.nom_municipio as nom_municipio_emp,etemp.nom_estado as nom_estado_emp,
	pemp.nom_pais as nom_pais_emp,upper(COALESCE(pj.nom_razao_social,pf.nom_pessoa)) as requerente,
    upper(COALESCE(pj2.nom_razao_social,pf2.nom_pessoa)) as procurador,
    upper(COALESCE(pj3.nom_razao_social,pf3.nom_pessoa)) as representante,tpl.des_tipologia as nome_atividade,cn.cod_cnae,
coalesce(
			overlay(overlay(overlay(overlay(pj.num_cnpj placing '-' from 13 for 0) placing '/' from 9 for 0) placing '.' from 6 for 0) placing '.' from 3 for 0),
			overlay(overlay(overlay(pf.num_cpf placing '-' from 10 for 0) placing '.' from 7 for 0) placing '.' from 4 for 0)) as num_documento
from lac_erb le
	inner join lac lc on lc.ide_lac = le.ide_lac_erb
	inner join requerimento rq on lc.ide_requerimento = rq.ide_requerimento
	inner join requerimento_unico rqu on rqu.ide_requerimento_unico = rq.ide_requerimento
	inner join porte pt on pt.ide_porte = rqu.ide_porte
	inner join tipo_modalidade_erb tm on tm.ide_tipo_modalidade_erb = le.ide_tipo_modalidade_erb
	inner join requerimento_pessoa rp on rp.ide_requerimento = rq.ide_requerimento
	inner join pessoa p on p.ide_pessoa = rp.ide_pessoa
	left join pessoa_juridica pj on pj.ide_pessoa_juridica = p.ide_pessoa
	left join pessoa_fisica pf on pf.ide_pessoa_fisica = p.ide_pessoa
	left join empreendimento_requerimento er on er.ide_requerimento = rq.ide_requerimento
	left join empreendimento em on em.ide_empreendimento = er.ide_empreendimento
	inner join empreendimento_tipologia etl on etl.ide_empreendimento = em.ide_empreendimento
	left join pessoa_juridica_cnae pjc on pjc.ide_pessoa_juridica = pj.ide_pessoa_juridica
	left join cnae cn on cn.ide_cnae = pjc.ide_cnae
	inner join tipologia_grupo tg on tg.ide_tipologia_grupo = etl.ide_tipologia_grupo
	inner join tipologia tpl on tpl.ide_tipologia = tg.ide_tipologia

	left join endereco_empreendimento eem on eem.ide_empreendimento = er.ide_empreendimento and eem.ide_tipo_endereco = 4
	left join endereco eemp on eemp.ide_endereco = eem.ide_endereco_empreendimento
	left join logradouro lemp on lemp.ide_logradouro = eemp.ide_logradouro
	left join tipo_logradouro tlemp on tlemp.ide_tipo_logradouro = lemp.ide_tipo_logradouro
	left join bairro bremp on bremp.ide_bairro = lemp.ide_bairro
	left join municipio mpemp on mpemp.ide_municipio = lemp.ide_municipio
	left join estado etemp on etemp.ide_estado = mpemp.ide_estado
	left join pais pemp on pemp.ide_pais = etemp.ide_pais

	left join endereco_pessoa ep on ep.ide_pessoa = em.ide_pessoa AND ep.ide_tipo_endereco = 1
	left join endereco ereq on ereq.ide_endereco = ep.ide_endereco_pessoa
	left join logradouro lreq on lreq.ide_logradouro = ereq.ide_logradouro
	left join tipo_logradouro tlreq on tlreq.ide_tipo_logradouro = lreq.ide_tipo_logradouro
	left join bairro brreq on brreq.ide_bairro = lreq.ide_bairro
	left join municipio mpreq on mpreq.ide_municipio = lreq.ide_municipio
	left join estado etreq on etreq.ide_estado = mpreq.ide_estado
	left join pais preq on preq.ide_pais = etreq.ide_pais


    left join requerimento_pessoa rp2 on rp2.ide_requerimento = rq.ide_requerimento and rp2.ide_tipo_pessoa_requerimento = 2
	left join pessoa p2 on p2.ide_pessoa = rp2.ide_pessoa
	left join pessoa_juridica pj2 on pj2.ide_pessoa_juridica = p.ide_pessoa
	left join pessoa_fisica pf2 on pf2.ide_pessoa_fisica = p.ide_pessoa

    left join requerimento_pessoa rp3 on rp3.ide_requerimento = rq.ide_requerimento and rp3.ide_tipo_pessoa_requerimento = 3
	left join pessoa p3 on p3.ide_pessoa = rp3.ide_pessoa
	left join pessoa_juridica pj3 on pj3.ide_pessoa_juridica = p3.ide_pessoa
	left join pessoa_fisica pf3 on pf3.ide_pessoa_fisica = p3.ide_pessoa


where le.ide_lac_erb = $P{ide_lac} and p.ind_excluido = false and rp.ide_tipo_pessoa_requerimento =1]]>
	</queryString>
