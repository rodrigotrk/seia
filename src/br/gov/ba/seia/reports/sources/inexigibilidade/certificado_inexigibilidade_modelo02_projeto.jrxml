left join municipio mgr on mgr.ide_municipio = lgr.ide_municipio
left join estado ufgr on ufgr.ide_estado = mgr.ide_estado
where ide_declaracao_inexigibilidade = $P{ide_declaracao_inexigibilidade}]]>
