	<queryString>
		<![CDATA[select distinct
	pe.ide_pessoa,
	pe.des_email,
	pf.ide_pessoa_fisica,
	initcap(pf.nom_pessoa) as nom_pessoa,
	pf.num_cpf,pf.dtc_nascimento,
	pf.des_naturalidade,
	papf.nom_pais as nom_pais_pf,
	pj.ide_pessoa_juridica,
	initcap(pj.nom_razao_social) as nom_razao_social,
	pj.num_cnpj,
	initcap(pj.nom_nome_fantasia) as nom_fantasia,
	pj.dtc_abertura,
	pj.num_inscricao_municipal,
	pj.num_inscricao_estadual,
	nj.nom_natureza_juridica,
	pa.nom_pais,
	l.nom_logradouro,
	tl.nom_tipo_logradouro,
	e.num_endereco,
	b.nom_bairro,
	e.des_complemento,
	m.nom_municipio,
	estado.des_sigla,
	e.des_ponto_referencia,
	case when length(trim(to_char(l.num_cep,'99999999')))=7 then '0'||trim(to_char(l.num_cep,'99999999')) else
	to_char(l.num_cep,'99999999') end as num_cep,
	estado.des_sigla as uf,
	emp.ide_empreendimento,
	csg.ide_classificacao_secao,
	csg.nom_classificacao_secao,
	p.nom_porte,
	fe.nom_fase_empreendimento,
	r.ide_requerimento,
	r.num_requerimento,
	emp.ind_cessionario,
	coalesce(emp.ind_conversao_tcra_lac,false) as ind_conversao_tcra_lac,
	r.ide_endereco_contato,
	r.nom_contato,
	r.num_telefone as num_telefone_contato,
	r.des_email as des_email_contato,
	sc.nom_sistema_coordenada,
	lg.ide_localizacao_geografica,
	lg.des_localizacao_geografica,
	coalesce(overlay(overlay(overlay(overlay(pj.num_cnpj placing '-' from 13 for 0) placing '/' from 9 for 0) placing '.' from 6 for 0) placing '.' from 3 for 0),
		overlay(overlay(overlay(pf.num_cpf placing '-' from 10 for 0) placing '.' from 7 for 0) placing '.' from 4 for 0)) as num_documento,r.dtc_criacao,csg.nom_classificacao_secao,array_to_string( array(
		SELECT DISTINCT
			tt."nom_tipo_telefone" ||': ' ||t."num_telefone"
		FROM
			"telefone_pessoa" tp INNER JOIN "telefone" t ON tp."ide_telefone" = t."ide_telefone"
			INNER JOIN "tipo_telefone" tt ON t."ide_tipo_telefone" = tt."ide_tipo_telefone"
		WHERE
			tp.ide_pessoa =  pe.ide_pessoa and t.ind_excluido = false)
			, ', ') as telefones,
	exists (select
			ir.ide_imovel_rural
		from
			imovel_empreendimento ie
			inner join imovel_rural ir on ir.ide_imovel_rural = ie.ide_imovel
		where
			ide_empreendimento = emp.ide_empreendimento)as imovel_rural,
	exists (select
			1
		from
			responsavel_empreendimento re
		where
			re.ide_empreendimento = emp.ide_empreendimento) as existe_responsaveis,
	exists (select distinct
			ide_imovel_rural
		from
			imovel_rural ir
			LEFT JOIN imovel_empreendimento ie on ie.ide_imovel = ir.ide_imovel_rural
			LEFT JOIN empreendimento emp on emp.ide_empreendimento = ie.ide_empreendimento
			LEFT join empreendimento_requerimento er on  er.ide_empreendimento = ie.ide_empreendimento
		WHERE
			ie.ide_empreendimento = emp.ide_empreendimento   and er.ide_requerimento = r.ide_requerimento) as exist_imovel,


array_to_string( array(SELECT latitude FROM (
		SELECT
			case when length(trim(f_get_vertices(ide_localizacao_geografica)):: character varying) > 0
			then
				substring(
					( SELECT trim(f_get_vertices(ide_localizacao_geografica) ) )
					FROM (strpos((SELECT trim(f_get_vertices(ide_localizacao_geografica))), '(')
					+ 1) for (strpos((SELECT trim(f_get_vertices(ide_localizacao_geografica))),
					' ') - 1)
					)
			else 'valor não informado.' end AS latitude
		FROM
			localizacao_geografica
		WHERE
			ide_localizacao_geografica = lg.ide_localizacao_geografica) pontos), ', ') AS latitude,


array_to_string( array(SELECT longitude FROM (
		SELECT
			case when length(trim(f_get_vertices(ide_localizacao_geografica)):: character varying) > 0
			then
			replace(substring((SELECT trim(f_get_vertices(ide_localizacao_geografica))) FROM (strpos((SELECT 			trim(f_get_vertices(ide_localizacao_geografica))), ' ') + 1) for (strpos((SELECT 					trim(f_get_vertices(ide_localizacao_geografica))), ')') - 1)), ')', '') else 'valor não 				informado.' end AS longitude

		FROM
			localizacao_geografica
		WHERE
			ide_localizacao_geografica = lg.ide_localizacao_geografica) pontos), ', ') AS longitude,

	exists(SELECT
			pfp.nom_pessoa as nom_pessoa_procurador_pessoa_fisica,
			pfrl.nom_pessoa as nom_pessoa_representante_legal,
			pfr.nom_pessoa as nom_pessoa_procurador_representante
		from
			pessoa pea
			LEFT JOIN pessoa_fisica pf ON pf.ide_pessoa_fisica = pea.ide_pessoa
			LEFT JOIN pessoa_juridica pj ON pj.ide_pessoa_juridica = pea.ide_pessoa
			LEFT JOIN procurador_pessoa_fisica ppf on ppf.ide_pessoa_fisica = pf.ide_pessoa_fisica
			LEFT JOIN pessoa_fisica pfp on pfp.ide_pessoa_fisica = ppf.ide_procurador
			LEFT JOIN procurador_representante pr on pr.ide_pessoa_juridica = pj.ide_pessoa_juridica
			LEFT JOIN pessoa_fisica pfr on pfr.ide_pessoa_fisica = pr.ide_procurador
			LEFT JOIN representante_legal rl on rl.ide_pessoa_juridica = pj.ide_pessoa_juridica
			LEFT JOIN pessoa_fisica pfrl on pfrl.ide_pessoa_fisica = rl.ide_pessoa_fisica
		WHERE
			pea.ide_pessoa = pe.ide_pessoa
			AND   pfp.ide_pessoa_fisica is not null) as exist_assinatura,
	exists (select
			t.cod_tipologia
		from
			requerimento_tipologia rt
			left join unidade_medida_tipologia_grupo umtg on umtg.ide_unidade_medida_tipologia_grupo = rt.ide_unidade_medida_tipologia_grupo
			left join unidade_medida um on um.ide_unidade_medida = umtg.ide_unidade_medida
			inner join tipologia_grupo tg on tg.ide_tipologia_grupo = umtg.ide_tipologia_grupo
			inner join tipologia t on t.ide_tipologia = tg.ide_tipologia
		where
			rt.ide_requerimento = $P{ide_requerimento}) as exist_atividade

from
	requerimento r
	INNER JOIN requerimento_unico ru on ru.ide_requerimento_unico = r.ide_requerimento
	INNER JOIN porte p on p.ide_porte = ru.ide_porte
	INNER JOIN fase_empreendimento fe on fe.ide_fase_empreendimento = ru.ide_fase_empreendimento
	INNER JOIN empreendimento_requerimento er ON er.ide_requerimento = r.ide_requerimento
	INNER JOIN empreendimento emp ON emp.ide_empreendimento = er.ide_empreendimento
	LEFT JOIN localizacao_geografica lg on lg.ide_localizacao_geografica = emp.ide_localizacao_geografica
	LEFT JOIN dado_geografico dg on dg.ide_localizacao_geografica = emp.ide_localizacao_geografica
	LEFT JOIN classificacao_secao_geometrica csg on csg.ide_classificacao_secao  = lg.ide_classificacao_secao
	LEFT JOIN sistema_coordenada sc on sc.ide_sistema_coordenada =lg.ide_sistema_coordenada
	INNER JOIN requerimento_pessoa rp ON rp.ide_requerimento = r.ide_requerimento and rp.ide_tipo_pessoa_requerimento = 1
	INNER JOIN pessoa pe ON pe.ide_pessoa = rp.ide_pessoa
	INNER JOIN endereco_pessoa ep on ep.ide_pessoa = pe.ide_pessoa
	INNER JOIN endereco e on e.ide_endereco = ep.ide_endereco
	INNER JOIN logradouro l ON l.ide_logradouro = e.ide_logradouro
	INNER JOIN tipo_logradouro tl ON l.ide_tipo_logradouro = tl.ide_tipo_logradouro
	INNER JOIN bairro b ON b.ide_bairro = l.ide_bairro
	INNER JOIN municipio m ON m.ide_municipio = l.ide_municipio
	INNER JOIN estado estado on estado.ide_estado = m.ide_estado
	INNER JOIN pais pa on pa.ide_pais = estado.ide_pais
	LEFT JOIN pessoa_fisica pf ON pf.ide_pessoa_fisica = pe.ide_pessoa
	LEFT JOIN pais papf on papf.ide_pais = pf.ide_pais
	LEFT JOIN pessoa_juridica pj ON pj.ide_pessoa_juridica = pe.ide_pessoa
	LEFT JOIN natureza_juridica nj on nj.ide_natureza_juridica = pj.ide_natureza_juridica
WHERE
	r.ide_requerimento = $P{ide_requerimento}]]>
	</queryString>
