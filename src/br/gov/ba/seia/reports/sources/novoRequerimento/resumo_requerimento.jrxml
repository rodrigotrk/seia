	<queryString>
		<![CDATA[select distinct
	ep.ide_endereco_pessoa ,

	r.dtc_criacao,
	pe.ide_pessoa,
	pe.des_email,
	pf.ide_pessoa_fisica,
	initcap(pf.nom_pessoa) as nom_pessoa,
	pf.num_cpf,pf.dtc_nascimento,
	pf.des_naturalidade,
	papf.nom_pais as nom_pais_pf,
	pj.ide_pessoa_juridica,
	initcap(pj.nom_razao_social) as nom_razao_social,
	pj.num_cnpj,
	initcap(pj.nom_nome_fantasia) as nom_fantasia,
	pj.dtc_abertura,
	pj.num_inscricao_municipal,
	pj.num_inscricao_estadual,
	nj.nom_natureza_juridica,
	pa.nom_pais,
	l.nom_logradouro,
	tl.nom_tipo_logradouro,
	e.num_endereco,
	b.nom_bairro,
	e.des_complemento,
	m.nom_municipio,
	estado.des_sigla,
	e.des_ponto_referencia,
	case when length(trim(to_char(l.num_cep,'99999999')))=7 then '0'||trim(to_char(l.num_cep,'99999999')) else
	to_char(l.num_cep,'99999999') end as num_cep,
	estado.des_sigla as uf,
	emp.ide_empreendimento,
	emp.nom_empreendimento,
	p.nom_porte,
	fe.nom_fase_empreendimento,
	r.ide_requerimento,
	r.num_requerimento,
	emp.ind_cessionario,
	coalesce(emp.ind_conversao_tcra_lac,false) as ind_conversao_tcra_lac,
	r.ide_endereco_contato,
	r.nom_contato,
	r.num_telefone as num_telefone_contato,
	r.des_email as des_email_contato,array_to_string( array(
		SELECT DISTINCT
			tt."nom_tipo_telefone" ||': ' ||t."num_telefone"
		FROM
			"telefone_pessoa" tp INNER JOIN "telefone" t ON tp."ide_telefone" = t."ide_telefone"
			INNER JOIN "tipo_telefone" tt ON t."ide_tipo_telefone" = tt."ide_tipo_telefone"
		WHERE
			tp.ide_pessoa =  pe.ide_pessoa and t."ind_excluido" = false)
			, ', ') as telefones,coalesce(overlay(overlay(overlay(overlay(pj.num_cnpj placing '-' from 13 for 0) placing '/' from 9 for 0) placing '.' from 6 for 0) placing '.' from 3 for 0),
		overlay(overlay(overlay(pf.num_cpf placing '-' from 10 for 0) placing '.' from 7 for 0) placing '.' from 4 for 0)) as num_documento
,f_get_resposta(63,r.ide_requerimento)::boolean as is_ARLS,
f_get_resposta(9,r.ide_requerimento)::boolean as is_TLA,
sa.ide_tipo_solicitacao,
sa.num_processo as num_processo_concluido,
s.nom_sistema as nom_sistema_processo_concluido,
initCap(sa.nom_razao_social_nova) as nom_razao_social_nova,
crf.ide_cumprimento_reposicao_florestal
from
	requerimento r
	LEFT JOIN requerimento_unico ru on ru.ide_requerimento_unico = r.ide_requerimento
	LEFT JOIN porte p on p.ide_porte = ru.ide_porte
	LEFT JOIN fase_empreendimento fe on fe.ide_fase_empreendimento = ru.ide_fase_empreendimento
	LEFT JOIN empreendimento_requerimento er ON er.ide_requerimento = r.ide_requerimento
	LEFT JOIN empreendimento emp ON emp.ide_empreendimento = er.ide_empreendimento
	INNER JOIN requerimento_pessoa rp ON rp.ide_requerimento = r.ide_requerimento and rp.ide_tipo_pessoa_requerimento = 1
	INNER JOIN pessoa pe ON pe.ide_pessoa = rp.ide_pessoa
	LEFT JOIN endereco_pessoa ep on ep.ide_pessoa = pe.ide_pessoa
	LEFT JOIN endereco e on e.ide_endereco = ep.ide_endereco
	LEFT JOIN logradouro l ON l.ide_logradouro = e.ide_logradouro
	LEFT JOIN tipo_logradouro tl ON l.ide_tipo_logradouro = tl.ide_tipo_logradouro
	LEFT JOIN bairro b ON b.ide_bairro = l.ide_bairro
	LEFT JOIN municipio m ON m.ide_municipio = l.ide_municipio
	LEFT JOIN estado estado on estado.ide_estado = m.ide_estado
	LEFT JOIN pais pa on pa.ide_pais = estado.ide_pais
	LEFT JOIN pessoa_fisica pf ON pf.ide_pessoa_fisica = pe.ide_pessoa
	LEFT JOIN pais papf on papf.ide_pais = pf.ide_pais
	LEFT JOIN pessoa_juridica pj ON pj.ide_pessoa_juridica = pe.ide_pessoa
	LEFT JOIN natureza_juridica nj on nj.ide_natureza_juridica = pj.ide_natureza_juridica
	left join solicitacao_administrativo sa on sa.ide_requerimento = r.ide_requerimento
	left join sistema s on sa.ide_sistema = s.ide_sistema and sa.ide_tipo_solicitacao = 12
	left join cumprimento_reposicao_florestal crf on crf.ide_requerimento=r.ide_requerimento
where r.ide_requerimento =$P{ide_requerimento}
order by ep.ide_endereco_pessoa
desc limit 1]]>
	</queryString>
