	<parameter name="ide_requerimento" class="java.lang.Integer">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[select r.ide_requerimento
 from
requerimento r
left join requerimento_tipologia rt on rt.ide_requerimento = r.ide_requerimento
left join unidade_medida_tipologia_grupo umtg on rt.ide_unidade_medida_tipologia_grupo = umtg.ide_unidade_medida_tipologia_grupo
left join tipologia_grupo tg on tg.ide_tipologia_grupo = umtg.ide_tipologia_grupo
left join tipologia t on t.ide_tipologia = tg.ide_tipologia
left join licenca l on r.ide_requerimento = l.ide_requerimento

where t.ide_tipologia != 292 --tipologia x
and t.ide_tipologia   != 293 --tipologia y
and (l.ide_tipo_solicitacao in (1,2,3) OR l.ide_tipo_solicitacao IS NULL) -- nova licenca, renovacao, alteracao, vazio(APE)
and r.ide_requerimento = $P{ide_requerimento}

group by r.ide_requerimento
]]>
	</queryString>
	<field name="ide_requerimento" class="java.lang.Integer"/>
