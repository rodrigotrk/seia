	<queryString>
		<![CDATA[select distinct nom_ato_ambiental,num_portaria,des_denominacao
 from pergunta_requerimento  pr
	left outer join imovel_rural ir on pr.ide_imovel = ir.ide_imovel_rural
	inner join florestal f on f.ide_requerimento = pr.ide_requerimento
	inner join florestal_ato_ambiental faa on faa.ide_florestal = f.ide_florestal
	inner join ato_ambiental aa on aa.ide_ato_ambiental = faa.ide_ato_ambiental
where f.ide_requerimento = $P{ide_requerimento} and ide_pergunta in (60) and ind_resposta = true]]>
	</queryString>
