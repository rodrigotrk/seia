	<queryString>
		<![CDATA[select distinct
	ir.ide_imovel_rural,
	lg.ide_localizacao_geografica,
	num_itr,
	des_denominacao,
	ir.val_area || ' (ha)' as val_area,
	des_cartorio,
	des_comarca,
	des_livro,
	num_folha,
	ind_reserva_legal,
	num_matricula,
	num_registro,
	num_ccir,
	ind_passivo_ambiental,
	ind_para_aderido,
	ind_aderir_para,
	num_processo_para,
	ta.ide_tipo_arl,
	i.ide_imovel,
	dsc_tipo_arl,
	i.ind_excluido ,
	i.dtc_criacao,
	i.dtc_exclusao,
	emp.ide_empreendimento,
	nom_empreendimento,
	val_investimento,
	qtd_funcionarios,
	des_email,
	num_cadastro_tecnico_federal,
	dtc_validade_ctf,
	ind_correspondencia,
	pi.ide_imovel,
	ide_tipo_imovel,
	e.ide_endereco,
	l.ide_logradouro,
	num_endereco,
	rla.val_area val_area_reserva,
	des_complemento,
	des_ponto_referencia,
	tl.ide_tipo_logradouro,
	b.ide_bairro,
	nom_logradouro,
	l.ind_origem_correio,
	m.ide_municipio,
	l.num_cep as  num_cep,
	nom_bairro,
	b.ind_origem_correio,
	est.ide_estado,
	ide_tipo_municipio,
	ide_situacao_municipio,
	nom_municipio,
	m.num_cep as num_cep_mun,
	ide_pais,
	nom_estado,
	des_sigla,
	tvi.ide_tipo_vinculo_imovel,
	nom_tipo_vinculo_imovel,
	csg.nom_classificacao_secao,
	sc.nom_sistema_coordenada,
	lg.ide_localizacao_geografica,
	nom_sistema_coordenada,
	des_localizacao_geografica,rlt.num_processo,est.des_sigla as uf,tl.nom_tipo_logradouro,

array_to_string( array(SELECT latitude FROM (
		SELECT
			case when length(trim(f_get_vertices(ide_localizacao_geografica)):: character varying) > 0
			then
				substring(
					( SELECT trim(f_get_vertices(ide_localizacao_geografica) ) )
					FROM (strpos((SELECT trim(f_get_vertices(ide_localizacao_geografica))), '(')
					+ 1) for (strpos((SELECT trim(f_get_vertices(ide_localizacao_geografica))),
					' ') - 1)
					)
			else 'valor não informado.' end AS latitude
		FROM
			localizacao_geografica
		WHERE
			ide_localizacao_geografica = lg.ide_localizacao_geografica) pontos), ', ') AS latitude,

array_to_string( array(SELECT longitude FROM (
		SELECT
			case when length(trim(f_get_vertices(ide_localizacao_geografica)):: character varying) > 0
			then
			replace(substring((SELECT trim(f_get_vertices(ide_localizacao_geografica))) FROM (strpos((
			SELECT trim(f_get_vertices(ide_localizacao_geografica))), ' ') + 1) for (strpos((SELECT
			trim(f_get_vertices(ide_localizacao_geografica))), ')') - 1)), ')', '')
			else 'valor não informado.' end AS longitude

		FROM
			localizacao_geografica
		WHERE
			ide_localizacao_geografica = lg.ide_localizacao_geografica) pontos), ', ') AS longitude,

exists (select *
from pessoa_imovel pi
INNER JOIN tipo_vinculo_imovel tvi on tvi.ide_tipo_vinculo_imovel = pi.ide_tipo_vinculo_imovel
WHERE tvi.ide_tipo_vinculo_imovel = 1
AND pi.ide_imovel = ir.ide_imovel_rural) as exist_proprietario
from imovel_rural ir
LEFT JOIN reserva_legal rla on rla.ide_imovel_rural = ir.ide_imovel_rural
LEFT JOIN tipo_arl ta on ta.ide_tipo_arl = rla.ide_tipo_arl
LEFT JOIN reserva_legal_tramite rlt ON rlt.ide_imovel = ir.ide_imovel_rural
LEFT JOIN imovel_empreendimento ie on ie.ide_imovel = ir.ide_imovel_rural
left join requerimento_imovel ri on ri.ide_imovel = ir.ide_imovel_rural and ri.ide_imovel = ir.ide_imovel_rural
and ide_tipo_imovel_requerimento = 1
LEFT JOIN empreendimento emp on emp.ide_empreendimento = ie.ide_empreendimento
LEFT join empreendimento_requerimento er on  er.ide_empreendimento = ie.ide_empreendimento
LEFT JOIN imovel i on i.ide_imovel = ie.ide_imovel
LEFT JOIN (select i.ide_imovel,pi.ide_tipo_vinculo_imovel from pessoa_imovel pi ,imovel i
	where pi.ide_imovel = i.ide_imovel
	and pi.ide_pessoa_imovel in (
	select max(pi.ide_pessoa_imovel) from pessoa_imovel pi ,imovel i
	where pi.ide_imovel = i.ide_imovel and pi.ide_tipo_vinculo_imovel = 1 group by i.ide_imovel
	)
group by i.ide_imovel,pi.ide_tipo_vinculo_imovel) pi on pi.ide_imovel = i.ide_imovel
LEFT JOIN endereco e on e.ide_endereco = i.ide_endereco
LEFT JOIN logradouro l on l.ide_logradouro = e.ide_logradouro
LEFT JOIN bairro b ON b.ide_bairro = l.ide_bairro
LEFT JOIN municipio m ON m.ide_municipio = l.ide_municipio
LEFT JOIN estado est on est.ide_estado = m.ide_estado
LEFT JOIN tipo_logradouro tl ON l.ide_tipo_logradouro = tl.ide_tipo_logradouro
LEFT JOIN tipo_vinculo_imovel tvi on tvi.ide_tipo_vinculo_imovel = pi.ide_tipo_vinculo_imovel
LEFT JOIN localizacao_geografica lg on lg.ide_localizacao_geografica = ir.ide_localizacao_geografica
LEFT JOIN dado_geografico dg on lg.ide_localizacao_geografica = dg.ide_localizacao_geografica
LEFT JOIN classificacao_secao_geometrica csg on csg.ide_classificacao_secao = lg.ide_classificacao_secao
LEFT JOIN sistema_coordenada sc on sc.ide_sistema_coordenada = lg.ide_sistema_coordenada
WHERE (ie.ide_empreendimento = $P{ide_empreendimento} and er.ide_requerimento = $P{requerimento})]]>
