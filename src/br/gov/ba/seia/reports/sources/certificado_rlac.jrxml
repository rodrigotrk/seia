	<queryString>
		<![CDATA[select tl.nom_tipo_logradouro,
	l.nom_logradouro,
	l.num_cep,
	e.num_endereco,
	br.nom_bairro,
	mp.nom_municipio,
	et.nom_estado,
	et.des_sigla,
	ps.nom_pais,
	rq.ide_requerimento,
	rq.num_requerimento,
	fe.nom_fase_empreendimento,
	f_get_vertices(lg.ide_localizacao_geografica) as vertices,
	ct.dtc_emissao_certificado,
	ct.num_certificado,
	trim(num_processo) as num_processo,
	tpl.ide_tipologia as tipologia,
	tpl.des_tipologia as nome_atividade,
	rtl.val_atividade,
	pf.nom_pessoa,
	pf.num_cpf,
	pj.nom_razao_social,
	pj.num_cnpj,
	lpc.ind_area_indigena,
	lpc.ind_sitio_arqueologico,
	lpc.ind_flutuante,
	lpc.ide_lac_posto_combustivel,
	lpc.ind_reformado,
	lt.ind_base_operacional,
	lt.ind_residuo,
	tr.dtc_movimentacao as dataProcessoFormado,
	overlay(overlay(overlay(num_token placing '-' from 25 for 0) placing '-' from 17 for 0) placing '-' from 9 for 0) as num_token,

	(
		select lac.ide_lac from lac lac
		where lac.ide_requerimento =
		(
			select proc.ide_requerimento from processo proc
			where proc.num_processo like
			(
				select lic.num_processo_licenca
				from requerimento req
				inner join licenca lic on lic.ide_requerimento = req.ide_requerimento
				where req.ide_requerimento = $P{ide_requerimento}
				AND lic.ide_tipo_solicitacao in (1,2,3) AND (lic.ind_excluido =FALSE OR lic.ind_excluido IS NULL)
			)
		)
	) as ide_lac,

	CASE
	 WHEN pf.nom_pessoa is not null
	 THEN initcap(pf.nom_pessoa)
	 ELSE initcap(pj.nom_razao_social)
	END as nome_razao_social,

	CASE
	 WHEN pf.nom_pessoa is not null
	 THEN pf.num_cpf
	 ELSE pj.num_cnpj
	END as num_cpf_cnpj,

	CASE
	 WHEN mp.nom_municipio is null
	 THEN 'não informado'
	 ELSE tl.nom_tipo_logradouro || ' ' || l.nom_logradouro
	 || coalesce(', ' || e.des_complemento, '')
	 || coalesce(', N°' || e.num_endereco || ', ', ', ')
	 || br.nom_bairro || ', '
	 || mp.nom_municipio || '/' || et.des_sigla
	END as endereco,

	tg.ind_excluido,
	tg.ide_tipologia_grupo

from requerimento rq
inner join processo prc on prc.ide_requerimento = rq.ide_requerimento
inner join certificado ct on ct.ide_requerimento = rq.ide_requerimento
left join tramitacao_requerimento tr ON tr.ide_requerimento = rq.ide_requerimento

inner join empreendimento_requerimento er on er.ide_requerimento = rq.ide_requerimento
inner join fase_empreendimento fe on fe.ide_fase_empreendimento = er.ide_fase_empreendimento
inner join empreendimento emp on emp.ide_empreendimento = er.ide_empreendimento
inner join porte pt on pt.ide_porte = er.ide_porte

inner join requerimento_tipologia rtl on rtl.ide_requerimento = rq.ide_requerimento
left join unidade_medida_tipologia_grupo umtg on umtg.ide_unidade_medida_tipologia_grupo = rtl.ide_unidade_medida_tipologia_grupo
left join tipologia_grupo tg on tg.ide_tipologia_grupo =umtg.ide_tipologia_grupo
left join tipologia tpl on tpl.ide_tipologia = tg.ide_tipologia

inner join requerimento_pessoa rp on rp.ide_requerimento = rq.ide_requerimento
inner join pessoa p on p.ide_pessoa = rp.ide_pessoa
left join pessoa_juridica pj on pj.ide_pessoa_juridica = p.ide_pessoa
left join pessoa_fisica pf on pf.ide_pessoa_fisica = p.ide_pessoa

left join endereco_empreendimento eem on eem.ide_empreendimento = emp.ide_empreendimento
left join localizacao_geografica lg on lg.ide_localizacao_geografica = emp.ide_localizacao_geografica
left join endereco e on e.ide_endereco = eem.ide_endereco
left join logradouro l on l.ide_logradouro = e.ide_logradouro
left join tipo_logradouro tl on tl.ide_tipo_logradouro = l.ide_tipo_logradouro
left join bairro br on br.ide_bairro = l.ide_bairro
left join municipio mp on mp.ide_municipio = l.ide_municipio
left join estado et on et.ide_estado = mp.ide_estado
left join pais ps on ps.ide_pais = et.ide_pais

left join lac_posto_combustivel lpc on lpc.ide_lac_posto_combustivel =
(
	select lac.ide_lac from lac lac
	where lac.ide_requerimento =
	(
		select proc.ide_requerimento from processo proc
		where proc.num_processo like
		(
			select lic.num_processo_licenca
			from requerimento req
			inner join licenca lic on lic.ide_requerimento = req.ide_requerimento
			where req.ide_requerimento = $P{ide_requerimento}
			AND lic.ide_tipo_solicitacao in (1,2,3) AND (lic.ind_excluido =FALSE OR lic.ind_excluido IS NULL)
		)
	)
)

left join lac_transporte lt on lt.ide_lac =
(
	select lac.ide_lac from lac lac
	where lac.ide_requerimento =
	(
		select proc.ide_requerimento from processo proc
		where proc.num_processo like
		(
			select lic.num_processo_licenca
			from requerimento req
			inner join licenca lic on lic.ide_requerimento = req.ide_requerimento
			where req.ide_requerimento = $P{ide_requerimento}
			AND lic.ide_tipo_solicitacao in (1,2,3) AND (lic.ind_excluido =FALSE OR lic.ind_excluido IS NULL)
		)
	)
)

where
rq.ide_requerimento = $P{ide_requerimento}
and rp.ide_tipo_pessoa_requerimento = 1
and rtl.ind_tipologia_principal = true
and eem.ide_tipo_endereco = 4
and tr.ide_status_requerimento = 8

order by tg.ind_excluido, tg.ide_tipologia_grupo limit 1;]]>
